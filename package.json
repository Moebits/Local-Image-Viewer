{
  "name": "localimageviewer",
  "version": "1.0.0",
  "description": "A local image viewer.",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm run clean && webpack && node dist/server.js",
    "build": "npm run clean && webpack && npm run redirects && npm run headers && npm run favicon && npm run json",
    "redirects": "echo \"/* /index.html 200\" | cat > dist/_redirects",
    "headers": "echo \"/*\nCross-Origin-Opener-Policy: same-origin\nCross-Origin-Embedder-Policy: require-corp\" | cat > dist/_headers",
    "favicon": "cp assets/icons/favicon.png dist/assets/icons/favicon.png",
    "server": "webpack && npm run pm2",
    "pm2": "pm2 stop all && pm2 start dist/server.js",
    "clean": "del-cli ./dist",
    "json": "node postdeploy.js"
  },
  "keywords": [
    "image",
    "viewer"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Tenpi/Local-Image-Viewer"
  },
  "author": "Moepi",
  "license": "MIT",
  "devDependencies": {
    "@tenpi/tslint": "^1.1.0",
    "@types/less": "^3.0.1",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^14.6.2",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/react-slider": "^1.3.1",
    "@types/webpack-env": "^1.15.2",
    "browserify-zlib": "^0.2.0",
    "copy-webpack-plugin": "^4.5.4",
    "crypto-browserify": "^3.12.0",
    "css-loader": "^4.2.2",
    "del-cli": "^3.0.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^8.0.1",
    "express-favicon": "^2.0.1",
    "file-loader": "^6.0.0",
    "fork-ts-checker-webpack-plugin": "^5.1.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^5.5.1",
    "less": "^3.12.2",
    "less-loader": "^7.0.0",
    "mini-css-extract-plugin": "^0.11.0",
    "path-browserify": "^1.0.1",
    "raw-loader": "^4.0.1",
    "react-router-dom": "^5.2.0",
